{
    "version": "0.2.0",
    "configurations": [{
        "name": "Launch HAClient.html",
        "type": "chrome",
        "request": "launch",
        "file": "${workspaceRoot}/HAClient.html"
    }, {
        "name": "Launch localhost with sourcemaps",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost/HAClient.html",
        "sourceMaps": true,
        "webRoot": "wwwroot"
    }, {
        "name": "Attach",
        "type": "chrome",
        "request": "attach",
        "port": 9222
    }]
}